{
  "name": "cdj_server",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "parser-readline": "^1.0.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "redux": "^4.0.5",
    "serialport": "^8.0.5",
    "socket.io": "^2.3.0",
    "pigpio": "3.0.0"
  },
  "scripts": {
    "start": "node ./serverbuild/index.js",
    "start_server": "node ./serverbuild/index.js",
    "build_server": "tsc -p ./server/tsconfig.json",
    "watch_server": "tsc -w -p ./server/tsconfig.json",
    "test_server": "jest --config ./server/jest.config.js",
    "monitor_server": "nodemon ./serverbuild/index.js",
    "start_website": "react-scripts start",
    "build_website": "react-scripts build",
    "test_website": "react-scripts test",
    "build_all": "npm run build_server && npm run build_website"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.12",
    "@types/pigpio": "^1.2.2",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux": "^3.6.0",
    "@types/socket.io": "^2.1.4",
    "dotenv": "^8.1.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^25.2.0",
    "typescript": "^3.6.4"
  }
}
